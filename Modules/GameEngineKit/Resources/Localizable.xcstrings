        "game_engine_kit.discover_leka_view.player.instructions": {
            "comment": "DiscoverLekaView instructions",
            "extractionState": "extracted_with_value",
            "localizations": {
                "en": {
                    "stringUnit": {
                        "state": "new",
                        "value": "\"Discover Leka\" allows the accompanied person to become familiar with Leka\nbefore even entering into learning Activities and Curriculums.\nThe robot will come to life, taking pauses so that the care receiver can\ntame your new companion!"
                    }
                },
                "fr": {
                    "stringUnit": {
                        "state": "translated",
                        "value": "\"Découvre Leka\" permet à la personne accompagnée de se familiariser à Leka\navant même d'entrer dans les apprentissages des Activités et des Parcours.\nLe robot va s'animer en faisant des pauses afin que\nla personne accompagnée puisse apprivoiser son nouveau compagnon !"
                    }
                }
            }
        },
