        "game_engine_kit.hide_and_seek_view.player.instructions": {
            "comment": "HideAndSeekView Player instructions",
            "extractionState": "extracted_with_value",
            "localizations": {
                "en": {
                    "stringUnit": {
                        "state": "new",
                        "value": "Encourage the care receiver to seek Leka.\nYou can throw a reinforcer to give him\na visual and/or audible clue.\nPress FOUND! once the robot found."
                    }
                },
                "fr": {
                    "stringUnit": {
                        "state": "translated",
                        "value": "Incites la personne accompagnée à chercher Leka.\nTu peux lancer un renforçateur pour lui donner un indice visuel et/ou sonore.\nAppuies sur \"TROUVÉ\" une fois le robot trouvé."
