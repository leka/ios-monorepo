        "game_engine_kit.hide_and_seek_view.player.instructions": {
            "comment": "HideAndSeekView Player instructions",
            "localizations": {
                "en": {
                    "stringUnit": {
                        "state": "translated",
                        "value": "Encourage the care receiver to seek Leka.\nYou can throw a reinforcer to give him\na visual and/or audible clue.\nPress FOUND! once the robot found."
                    }
                },
                "fr": {
                    "stringUnit": {
                        "state": "translated",
                        "value": "Incite la personne accompagn\u00e9e \u00e0 chercher Leka.\nTu peux lancer un renfor\u00e7ateur pour lui donner un indice visuel et/ou sonore.\nAppuie sur \"TROUV\u00c9\" une fois le robot trouv\u00e9."
                    }
                }
            }
        },
