{
    "files.associations": {
        "*.xcstrings": "json"
    },
    "[xcstrings]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.formatOnSave": true,
    "xo.enable": true,
    "xo.format.enable": true,
    "[javascript]": {
        "editor.defaultFormatter": "samverschueren.linter-xo"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "samverschueren.linter-xo"
    },
    "[typescript]": {
        "editor.defaultFormatter": "samverschueren.linter-xo"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "samverschueren.linter-xo"
    }
}
